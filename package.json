{
  "name": "react-graphql-blog",
  "private": true,
  "version": "0.1.0",
  "description": "An example of a blog made with React and GraphQL",
  "main": "./dist/server/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.prod.js",
    "build:server": "tsc -p tsconfig.server.json",
    "build:start": "npm run build && npm run serve",
    "serve": "node ./dist/server/index.js",
    "serve:watch": "nodemon",
    "start": "webpack-dev-server --open --port 3333",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Dan Nguyen <nghuudan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/enzyme": "^3.1.12",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.3.0",
    "ajv": "^6.5.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "jest-transform-graphql": "^2.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link-http": "^1.5.4",
    "apollo-link-schema": "^1.1.0",
    "apollo-server-express": "^2.0.0",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "graphql": "0.13.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "material-icons": "^0.2.3",
    "mobx": "^5.0.3",
    "react": "^16.4.0",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "rmwc": "^1.8.3",
    "roboto-fontface": "^0.9.0"
  }
}
